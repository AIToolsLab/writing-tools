{
	"name": "office-addin-taskpane-js",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/AIToolsLab/writing-tools"
	},
	"license": "MIT",
	"config": {
		"app_to_debug": "word",
		"app_type_to_debug": "desktop",
		"dev_server_port": 3000
	},
	"scripts": {
		"build": "webpack --mode production",
		"build:dev": "webpack --mode development",
		"dev-server": "webpack serve --mode development",
		"start": "office-addin-debugging start manifest.xml",
		"start:desktop": "office-addin-debugging start manifest.xml desktop",
		"start:web": "office-addin-debugging start manifest.xml web",
		"stop": "office-addin-debugging stop manifest.xml",
		"validate": "office-addin-manifest validate manifest.xml",
		"validate:prod": "office-addin-manifest validate -p dist/manifest.xml",
		"watch": "webpack --mode development --watch",
		"lint": "eslint \"**/*.{js,ts,tsx}\"",
		"lint:fix": "eslint --fix --ext .js,.ts,.tsx .",
		"prepare": "cd .. && husky install add-in/.husky"
	},
	"dependencies": {
		"@auth0/auth0-react": "^2.2.4",
		"@fluentui/react": "^8.52.3",
		"@lexical/react": "^0.16.1",
		"@microsoft/fetch-event-source": "^2.0.1",
		"@react-hook/window-size": "^3.1.1",
		"lexical": "^0.16.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-icons": "^5.2.1",
		"react-remark": "^2.1.0",
		"react-textarea-autosize": "^8.5.3"
	},
	"devDependencies": {
		"@babel/core": "^7.13.10",
		"@babel/preset-typescript": "^7.13.0",
		"@types/office-js": "^1.0.256",
		"@types/office-runtime": "^1.0.23",
		"@types/react": "^17.0.39",
		"@types/react-dom": "^17.0.11",
		"@types/webpack": "^4.4.34",
		"@typescript-eslint/eslint-plugin": "^6.5.0",
		"@typescript-eslint/parser": "^6.5.0",
		"acorn": "^8.5.0",
		"babel-loader": "^8.2.2",
		"copy-webpack-plugin": "^9.0.1",
		"core-js": "^3.37.1",
		"css-loader": "^6.8.1",
		"es6-promise": "^4.2.8",
		"eslint": "^8.48.0",
		"eslint-plugin-react": "^7.28.0",
		"file-loader": "^6.2.0",
		"html-loader": "^4.1.0",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^8.0.0",
		"less": "^3.9.0",
		"less-loader": "^10.0.1",
		"lint-staged": "^14.0.1",
		"office-addin-cli": "^1.5.5",
		"office-addin-debugging": "^5.0.5",
		"office-addin-dev-certs": "^1.11.3",
		"office-addin-manifest": "^1.12.3",
		"os-browserify": "^0.3.0",
		"process": "^0.11.10",
		"regenerator-runtime": "^0.13.7",
		"source-map-loader": "^3.0.0",
		"style-loader": "^3.3.3",
		"ts-loader": "^9.4.1",
		"typescript": "^4.3.5",
		"webpack": "^5.76.3",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "4.13.1"
	},
	"browserslist": [
		"ie 11"
	],
	"lint-staged": {
		"**/*.{js,ts,tsx}": [
			"yarn run lint"
		]
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
